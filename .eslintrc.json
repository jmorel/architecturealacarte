{
    "extends": [
        "airbnb",
        "react-app"
    ],
    "env": {
        "browser": true
    },
    "rules": {
        "indent": ["error", 4],
        "max-len": "off",
        "react/jsx-indent": [2, 4],
        "react/no-unescaped-entities": 0,
        "react/jsx-indent-props": [2, 4],
        "react/forbid-prop-types": 0,
        "react/no-multi-comp": 0,
        "jsx-a11y/href-no-hash": 0,
        "import/prefer-default-export": 0
    }
}